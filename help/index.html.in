<!DOCTYPE html>
<html lang="en" id="help">

<head>
    <title>Wiki pages</title>
    %INCLUDES%
</head>

<body class="chaise-body">
    <div class="configure-container" ng-app="chaise.configure-help">
        <loading-spinner></loading-spinner>
    </div>
    <div class="app-container help-container" ng-controller="helpController as ctrl">
        <navbar></navbar>
        <loading-spinner ng-show="!displayReady"></loading-spinner>
        <div class="app-content-container" ng-show="displayReady">
            <!-- <div class="top-panel-container">

            </div> -->
            <div class="bottom-panel-container">
                <div class="side-panel-resizable close-panel"></div>
                <div class="main-container">
                    <div class="main-body container">
                        <div class="row">
                            <div ng-bind-html="helpContent | trustedHTML" class="col-md-9 markdown-container help-content"></div>
                            <div class="col-md-3">
                                <nav class="hidden-sm hidden-xs toc" id="toc-container">
                                </nav>
                            </div>
                        </div>
                    </div>
                    <footer></footer>
                </div>
            </div>
        </div>
    </div>
</body>


</html>
